       "</div>\n",
       "</div>\n",
       "<script type=\"text/javascript\">\n",
       "\n",
       "    var currentListPage = 1;\n",
       "    var totalListPages = 1;\n",
       "\n",
       "        $( document ).ready(function() {\n",
       "\n",
       "        filterCourseList = function filterCourseList() {\n",
       "            var searchTerm = $(\"#search-view\").val();\n",
       "            filters = [];\n",
       "            var notSearchTagsFilter = []\n",
       "            $('.tag-filter-checkbox:checked').each(function (i, el) {\n",
       "                filters.push($(el).val());\n",
       "            });\n",
       "\n",
       "            $(window.searchTags).each(function (i, el) {\n",
       "               filters.push(el);\n",
       "            });\n",
       "            $(window.notSearchTags).each(function (i, el) {\n",
       "               notSearchTagsFilter.push(el);\n",
       "            });\n",
       "\n",
       "            filters = filters.filter(function(value) {\n",
       "                return value !== null;\n",
       "            });\n",
       "\n",
       "            $.ajax({\n",
       "                type: \"GET\",\n",
       "                url: courseApiEndpoint,\n",
       "                dataType: 'json',\n",
       "                headers: { },\n",
       "                data: {\n",
       "                    'sort' : defaultSortOption,\n",
       "                    'onlyFavourites' : window.onlyFavourites,\n",
       "                    'start' : 0,\n",
       "                    'length' : recordsPerListPage,\n",
       "                    'search' : {\n",
       "                        'value' : searchTerm,\n",
       "                        'regex' : false\n",
       "                    },\n",
       "                    'searchTags': [],\n",
       "                    'notSearchTags': [],\n",
       "                    'isNewCourses' : $(\"#new_tag\").prop('checked') ? 1 : 0,\n",
       "                    'strictTagSearch' : strictTagSearch,\n",
       "                    'inProgress': window.inProgress,\n",
       "                    'page' : {\n",
       "                        'size' : recordsPerListPage,\n",
       "                        'offset' : (recordsPerListPage*(currentListPage-1))\n",
       "                    },\n",
       "                    'filter' : {\n",
       "                        'searchTags' : filters,\n",
       "                        'notSearchTags' : notSearchTagsFilter,\n",
       "                        'isNew' : $(\"#new_tag\").prop('checked') ? 1 : 0,\n",
       "                    }\n",
       "                }\n",
       "            })\n",
       "            .done(function (response) {\n",
       "\n",
       "                if(response['data'] == ''){\n",
       "                    $(\"#course-list-tile-container\").html('<span class=\"text-center\">No Record Found!!!</span>');\n",
       "                }else{\n",
       "                    processResponse(response['data']);\n",
       "                }\n",
       "\n",
       "                processPagination(response['meta']['pagination']);\n",
       "\n",
       "            })\n",
       "            .fail(function (xhr, status, error) {\n",
       "                //failed the ajax call\n",
       "                new PNotify({\n",
       "                    title: 'Error!',\n",
       "                    text: 'Failed to Fetch Course List!',\n",
       "                    icon: 'icon-blocked',\n",
       "                    type: 'error'\n",
       "                });\n",
       "            });\n",
       "\n",
       "        }\n",
       "\n",
       "        function processResponse(response) {\n",
       "\n",
       "            var content = '';\n",
       "\n",
       "            for (var i = 0; i < response.length; i++) {\n",
       "\n",
       "                var tileData = response[i]['attributes'];\n",
       "                var mainTitle = tileData.title;\n",
       "                for (var tagIndex = 0; tagIndex < tileData.tags.length; tagIndex++) {\n",
       "                    var trimData = $.trim(tileData.tags[tagIndex].groupName);\n",
       "                    if(trimData.toLowerCase() == 'product' || trimData.toLowerCase() == 'application' ){\n",
       "                        mainTitle = tileData.tags[tagIndex].title;\n",
       "                    }\n",
       "                }\n",
       "\n",
       "                var courseImage = defaultImage;\n",
       "                if (tileData.imagePublicUrl !== null) {\n",
       "                    courseImage = tileData.imagePublicUrl;\n",
       "                }\n",
       "\n",
       "                $(\"#course-list-tile-container\").empty();\n",
       "                $(\"#course-list-tile-container\").fadeOut(500);\n",
       "\n",
       "                content += '<div class=\"col-md-12 mb-4 d-flex\">';\n",
       "                content += '<div class=\"cource-grid cource-list bg-white flex-1\">';\n",
       "                content += '<div class=\"img-section\"><img src=\"'+courseImage+'\" alt=\"\"></div>';\n",
       "                content += '<div class=\"cource-content\">';\n",
       "\n",
       "                var description = tileData.summary.replace(/<\\/?[^>]+(>|$)/g, \"\");\n",
       "                if (description.length > 100) {\n",
       "                    description = description.substring(0,100);\n",
       "                    description = description + '...';\n",
       "                }\n",
       "\n",
       "                content += '<span style=\"text-transform: uppercase;\">'+ mainTitle +'</span>';\n",
       "                content += '<a href=\"/courses/' + response[i].id + '\"><h5>'+ toTitleCase(tileData.title) +'</h5></a>';\n",
       "                content += '<p class=\"mb-2\">'+description+'</p>';\n",
       "\n",
       "                if(!tileData.duration){\n",
       "                    tileData.duration = '0:00';\n",
       "                }else{\n",
       "                    var hours = Math.floor(tileData.duration / 60);\n",
       "                    if (isNaN(hours)) {\n",
       "                        hours = '00';\n",
       "                    }\n",
       "                    var minutes = Math.floor(tileData.duration % 60);\n",
       "                    if (isNaN(minutes)) {\n",
       "                        minutes = '00';\n",
       "                    }\n",
       "                    tileData.duration = hours.toString().padStart(2, '0') + ':' + minutes.toString().padStart(2, '0');\n",
       "                }\n",
       "\n",
       "                content += '<div class=\"cource-tabs\">';\n",
       "                content += '<ul class=\"mb-0 ps-0 d-flex flex-wrap\">';\n",
       "\n",
       "                if($(\"#new_tag\").prop('checked')){\n",
       "                    content += '<li><a href=\"#\" class=\"bg-success text-white\">New</a></li>';\n",
       "                }\n",
       "\n",
       "                for (var tagIndex = 0; tagIndex < tileData.tags.length; tagIndex++) {\n",
       "                    content += '<li><a href=\"#\" class=\"tag-filter\" data-tag=\"'+ tileData.tags[tagIndex].title +'\">'+ tileData.tags[tagIndex].title +'</a></li>';\n",
       "                }\n",
       "\n",
       "                content += '</ul></div><hr>';\n",
       "                content += '<div class=\"d-flex flex-wrap justify-content-between align-items-center gap-2\">';\n",
       "                content += '<a href=\"/courses/' + response[i].id + '\" class=\"learn-more\"><span>Learn more</span></a>';\n",
       "                content += '<p class=\"mb-0\"><img src=\"/content/rockwell/images/certification/clock.png\" width=\"15px\" class=\"me-2\" alt=\"clock\">'+tileData.duration+'</p>';\n",
       "                content += '</div>';\n",
       "                content += '</div>';\n",
       "                content += '</div>';\n",
       "                content += '</div>';\n",
       "\n",
       "            }\n",
