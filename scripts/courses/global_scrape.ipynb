       "                var tagValue = $(this).val();\n",
       "                var isChecked = $.inArray(tagValue,filters) !== -1;\n",
       "                $(this).prop('checked', isChecked);\n",
       "                if (isChecked) {\n",
       "                    $(this).closest('span').addClass('checked');\n",
       "                } else {\n",
