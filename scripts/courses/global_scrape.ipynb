       "        }\n",
       "\n",
       "        function processPagination(paginationData) {\n",
       "\n",
       "            totalListPages = Math.ceil(paginationData['count'] / recordsPerListPage);\n",
       "\n",
       "            // Preparing pagination buttons\n",
       "            var buttonContent = '<ul class=\"pagination pagination-flat\">';\n",
       "\n",
       "            // Previous button\n",
       "            buttonContent += '<li class=\"' + (currentListPage === 1 ? 'disabled' : '') + '\"><a href=\"javascript:void(0);\" class=\"list-pagination-previous-btn\"><i class=\"icon-arrow-left12\"></i></a></li>';\n",
       "\n",
       "            // First page button\n",
       "            if (currentListPage > 4 && totalListPages > 6) {\n",
       "                buttonContent += '<li><a href=\"javascript:void(0);\" class=\"list-pagination-btn\" data-pageNo=\"1\">1</a></li>';\n",
       "                buttonContent += '<li class=\"disabled\"><a href=\"javascript:void(0);\">...</a></li>';\n",
       "            }\n",
       "\n",
       "            // Determine the start and end pages for the pagination buttons\n",
       "            var startPage = Math.max(1, currentListPage - 2);\n",
       "            var endPage = Math.min(totalListPages, currentListPage + 2);\n",
       "\n",
       "            // Adjust the range to always show 5 buttons if possible\n",
       "            if (currentListPage <= 4) {\n",
       "                startPage = 1;\n",
       "                endPage = Math.min(5, totalListPages);\n",
       "            } else if (currentListPage >= totalListPages - (totalListPages > 7 ? 3 : 2)) {\n",
       "                endPage = totalListPages;\n",
       "                startPage = Math.max(1, totalListPages - 4);\n",
       "            }\n",
       "\n",
       "            // Add page buttons\n",
       "            for (var i = startPage; i <= endPage; i++) {\n",
       "                var activePaginationClass = currentListPage === i ? 'active' : '';\n",
       "                buttonContent += '<li class=\"' + activePaginationClass + '\"><a href=\"javascript:void(0);\" class=\"list-pagination-btn\" data-pageNo=\"' + i + '\">' + i + '</a></li>';\n",
       "            }\n",
       "\n",
       "            // Last page button\n",
       "            if ((currentListPage < totalListPages - (totalListPages > 7 ? 3 : 2)) && totalListPages > 6) {\n",
       "                buttonContent += '<li class=\"disabled\"><a href=\"javascript:void(0);\">...</a></li>';\n",
       "                buttonContent += '<li><a href=\"javascript:void(0);\" class=\"list-pagination-btn\" data-pageNo=\"' + totalListPages + '\">' + totalListPages + '</a></li>';\n",
       "            }\n",
       "\n",
       "            // Next button\n",
       "            buttonContent += '<li class=\"' + (currentListPage === totalListPages ? 'disabled' : '') + '\"><a href=\"javascript:void(0);\" class=\"list-pagination-next-btn\"><i class=\"icon-arrow-right13\"></i></a></li>';\n",
       "\n",
       "            buttonContent += '</ul>';\n",
       "\n",
       "            $(\"#course-list-pagination-button-container\").html(buttonContent);\n",
       "            $(\"#header-course-list-pagination-button-container\").html(buttonContent);\n",
       "        }\n",
       "\n",
       "        $(document).on('click', \".list-pagination-btn\" , function (e) {\n",
       "\n",
       "            var page = $(this).attr('data-pageNo');\n",
       "\n",
       "            currentListPage = parseInt(page);\n",
       "\n",
       "            filterCourseList();\n",
       "        });\n",
       "\n",
       "        $(document).on('click', \".list-pagination-previous-btn\" , function (e) {\n",
       "\n",
       "            if (currentListPage > 1) {\n",
       "                currentListPage -= 1;\n",
       "                filterCourseList();\n",
       "            }\n",
       "        });\n",
       "\n",
       "        $(document).on('click', \".list-pagination-next-btn\" , function (e) {\n",
       "\n",
       "            if (currentListPage < totalListPages) {\n",
       "                currentListPage += 1;\n",
       "                filterCourseList();\n",
       "            }\n",
       "        });\n",
       "\n",
       "        // run on page ready\n",
       "        filterCourseList();\n",
       "\n",
       "        function toTitleCase(str) {\n",
       "            return str.replace(\n",
       "                /\\w\\S*/g,\n",
       "                text => text.charAt(0).toUpperCase() + text.substring(1).toLowerCase()\n",
       "            );\n",
       "        }\n",
       "\n",
       "    });\n",
       "\n",
       "</script>\n",
