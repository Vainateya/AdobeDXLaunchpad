       "                // search the course grid view\n",
       "                filterCourseGrid();\n",
       "                // search the course list view\n",
       "                filterCourseList();\n",
       "            }, 500));\n",
       "\n",
       "            function delayCounter(f, delay) {\n",
       "                var timer = null;\n",
       "                return function(){\n",
       "                    var context = this, args = arguments;\n",
       "                    clearTimeout(timer);\n",
       "                    timer = window.setTimeout(function(){\n",
       "                            f.apply(context, args);\n",
       "                        },\n",
       "                        delay || 500);\n",
       "                };\n",
       "            }\n",
       "\n",
       "        });\n",
