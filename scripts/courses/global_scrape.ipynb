       "                    },\n",
       "                    'filter' : {\n",
       "                        'searchTags' : filters,\n",
       "                        'notSearchTags' : notSearchTagsFilter,\n",
       "                        'isNew' : $(\"#new_tag\").prop('checked') ? 1 : 0,\n",
       "                    }\n",
       "                }\n",
       "            })\n",
       "            .done(function (response) {\n",
       "\n",
       "                 if(response['data'] == ''){\n",
       "                    $(\"#course-grid-tile-container\").html('<span class=\"text-center\">No Record Found!!!</span>');\n",
       "                }else{\n",
       "                    processResponse(response['data']);\n",
       "                }\n",
       "\n",
       "                processPagination(response['meta']['pagination']);\n",
       "\n",
       "            })\n",
       "            .fail(function (xhr, status, error) {\n",
       "                //failed the ajax call\n",
       "\n",
       "                new PNotify({\n",
       "                    title: 'Error!',\n",
       "                    text: 'Failed to Fetch Course Grid!',\n",
       "                    icon: 'icon-blocked',\n",
       "                    type: 'error'\n",
       "                });\n",
       "            });\n",
       "\n",
       "        }\n",
       "\n",
       "        function processResponse(response) {\n",
       "\n",
       "            var content = '';\n",
       "\n",
       "            for (var i = 0; i < response.length; i++) {\n",
       "\n",
       "                var tileData = response[i]['attributes'];\n",
       "                var mainTitle = tileData.title;\n",
       "                for (var tagIndex = 0; tagIndex < tileData.tags.length; tagIndex++) {\n",
       "                    var trimData = $.trim(tileData.tags[tagIndex].groupName);\n",
       "                    if(trimData.toLowerCase() == 'product' || trimData.toLowerCase() == 'application' ){\n",
       "                        mainTitle = tileData.tags[tagIndex].title;\n",
       "                        break;\n",
       "                    }\n",
       "                }\n",
       "\n",
       "                var courseImage = defaultImage;\n",
       "                if (tileData.imagePublicUrl !== null) {\n",
       "                    courseImage = tileData.imagePublicUrl;\n",
       "                }\n",
       "\n",
       "                $(\"#course-grid-tile-container\").empty();\n",
       "                $(\"#course-grid-tile-container\").fadeOut(500);\n",
       "\n",
       "                content += '<div class=\"col-md-6 col-lg-4 mb-4 d-flex\">';\n",
       "                content += '<div class=\"cource-grid d-flex flex-column flex-start bg-white flex-1\">';\n",
       "                content += '<div class=\"img-section\"><a href=\"/courses/' + response[i].id + '\"><img src=\"'+courseImage+'\" alt=\"\"></a></div>';\n",
       "\n",
       "                content += '<div class=\"cource-content d-flex flex-column justify-content-between\">';\n",
       "\n",
       "                var description = tileData.summary.replace(/<\\/?[^>]+(>|$)/g, \"\");\n",
       "                if (description.length > 100) {\n",
       "                    description = description.substring(0,100);\n",
       "                    description = description + '...';\n",
       "                }\n",
       "\n",
       "                content += '<div class=\"\"><span style=\"text-transform: uppercase;\">'+ mainTitle +'</span>';\n",
       "                content += '<a href=\"/courses/' + response[i].id + '\"><h5>'+ tileData.title +'</h5></a>';\n",
       "                content += '<a href=\"/courses/' + response[i].id + '\"><p class=\"mb-2\" style=\"color:#2C2C2C!important;\" title=\"'+description+'\">'+description+'</p></a>';\n",
       "\n",
       "                if(!tileData.duration){\n",
       "                    tileData.duration = '0:00';\n",
       "                }else{\n",
       "                    var hours = Math.floor(tileData.duration / 60);\n",
       "                    if (isNaN(hours)) {\n",
       "                        hours = '00';\n",
       "                    }\n",
       "                    var minutes = Math.floor(tileData.duration % 60);\n",
       "                    if (isNaN(minutes)) {\n",
       "                        minutes = '00';\n",
       "                    }\n",
       "                    tileData.duration = hours.toString().padStart(2, '0') + ':' + minutes.toString().padStart(2, '0');\n",
       "                }\n",
       "\n",
       "                content += '<div class=\"cource-tabs\">';\n",
       "                content += '<ul class=\"mb-0 ps-0 d-flex flex-wrap\">';\n",
       "\n",
       "                if($(\"#new_tag\").prop('checked')){\n",
       "                    content += '<li><a href=\"#\" class=\"bg-success text-white\">New</a></li>';\n",
       "                }\n",
       "\n",
       "                for (var tagIndex = 0; tagIndex < tileData.tags.length; tagIndex++) {\n",
       "                    content += '<li><a href=\"#\" class=\"tag-filter\" data-tag=\"'+ tileData.tags[tagIndex].title +'\">'+ tileData.tags[tagIndex].title +'</a></li>';\n",
       "                }\n",
       "\n",
       "               content += '</ul></div></div>';\n",
       "                content += '<div><hr><div class=\"d-flex flex-wrap justify-content-between align-items-center gap-2\">';\n",
       "                content += '<a href=\"/courses/' + response[i].id + '\" class=\"learn-more\"><span>Learn more</span></a>';\n",
       "                content += '<p class=\"mb-0\"><img src=\"/content/rockwell/images/certification/clock.png\" width=\"15px\" class=\"me-2\" alt=\"clock\">'+tileData.duration+'</p>';\n",
       "                content += '</div></div>';\n",
       "                content += '</div>';\n",
       "                content += '</div>';\n",
       "                content += '</div>';\n",
       "\n",
       "            }\n",
       "\n",
       "            $(\"#course-grid-tile-container\").html(content);\n",
       "\n",
       "            $(\"#course-grid-tile-container\").fadeIn(500);\n",
       "        }\n",
       "\n",
       "        function processPagination(paginationData) {\n",
       "\n",
       "            totalGridPages = Math.ceil(paginationData['count'] / recordsPerGridPage);\n",
       "\n",
       "            // Preparing pagination buttons\n",
       "            var buttonContent = '<ul class=\"pagination pagination-flat\">';\n",
       "\n",
       "            // Previous button\n",
       "            buttonContent += '<li class=\"' + (currentGridPage === 1 ? 'disabled' : '') + '\"><a href=\"javascript:void(0);\" class=\"grid-pagination-previous-btn\"><i class=\"icon-arrow-left12\"></i></a></li>';\n",
       "\n",
       "            // First page button\n",
       "            if (currentGridPage > 4 && totalGridPages > 6) {\n",
       "                buttonContent += '<li><a href=\"javascript:void(0);\" class=\"grid-pagination-btn\" data-pageNo=\"1\">1</a></li>';\n",
       "                buttonContent += '<li class=\"disabled\"><a href=\"javascript:void(0);\">...</a></li>';\n",
       "            }\n",
       "\n",
       "            // Determine the start and end pages for the pagination buttons\n",
       "            var startPage = Math.max(1, currentGridPage - 2);\n",
       "            var endPage = Math.min(totalGridPages, currentGridPage + 2);\n",
       "\n",
       "            // Adjust the range to always show 5 buttons if possible\n",
       "            if (currentGridPage <= 4) {\n",
       "                startPage = 1;\n",
       "                endPage = Math.min(5, totalGridPages);\n",
       "            } else if (currentGridPage >= totalGridPages - (totalGridPages > 7 ? 3 : 2)) {\n",
       "                endPage = totalGridPages;\n",
       "                startPage = Math.max(1, totalGridPages - 4);\n",
       "            }\n",
       "\n",
       "            // Add page buttons\n",
       "            for (var i = startPage; i <= endPage; i++) {\n",
       "                var activePaginationClass = currentGridPage === i ? 'active' : '';\n",
       "                buttonContent += '<li class=\"' + activePaginationClass + '\"><a href=\"javascript:void(0);\" class=\"grid-pagination-btn\" data-pageNo=\"' + i + '\">' + i + '</a></li>';\n",
       "            }\n",
       "\n",
       "            // Last page button\n",
       "            if ((currentGridPage < totalGridPages - (totalGridPages > 7 ? 3 : 2)) && totalGridPages > 6) {\n",
       "                buttonContent += '<li class=\"disabled\"><a href=\"javascript:void(0);\">...</a></li>';\n",
       "                buttonContent += '<li><a href=\"javascript:void(0);\" class=\"grid-pagination-btn\" data-pageNo=\"' + totalGridPages + '\">' + totalGridPages + '</a></li>';\n",
       "            }\n",
       "\n",
       "            // Next button\n",
       "            buttonContent += '<li class=\"' + (currentGridPage === totalGridPages ? 'disabled' : '') + '\"><a href=\"javascript:void(0);\" class=\"grid-pagination-next-btn\"><i class=\"icon-arrow-right13\"></i></a></li>';\n",
       "\n",
       "            buttonContent += '</ul>';\n",
       "\n",
       "            $(\"#course-grid-pagination-button-container\").html(buttonContent);\n",
       "            $(\"#header-course-grid-pagination-button-container\").html(buttonContent);\n",
       "        }\n",
       "\n",
       "        $(document).on('click', \".grid-pagination-btn\" , function (e) {\n",
       "\n",
       "            var page = $(this).attr('data-pageNo');\n",
       "\n",
       "            currentGridPage = parseInt(page);\n",
       "\n",
       "            filterCourseGrid();\n",
       "        });\n",
       "\n",
       "        $(document).on('click', \".grid-pagination-previous-btn\" , function (e) {\n",
       "\n",
       "            if (currentGridPage > 1) {\n",
       "                currentGridPage -= 1;\n",
       "                filterCourseGrid();\n",
       "            }\n",
       "        });\n",
       "\n",
       "        $(document).on('click', \".grid-pagination-next-btn\" , function (e) {\n",
       "\n",
       "            if (currentGridPage < totalGridPages) {\n",
       "                currentGridPage += 1;\n",
       "                filterCourseGrid();\n",
       "            }\n",
       "        });\n",
       "\n",
       "        // run on page ready\n",
       "        filterCourseGrid();\n",
       "\n",
       "        function toTitleCase(str) {\n",
       "            return str.replace(\n",
       "                /\\w\\S*/g,\n",
       "                text => text.charAt(0).toUpperCase() + text.substring(1).toLowerCase()\n",
       "            );\n",
       "        }\n",
       "\n",
       "    });\n",
       "\n",
       "</script>\n",
       "</div>\n",
       "</div>\n",
       "</div>\n",
       "</div>\n",
       "</div>\n",
       "</div>\n",
